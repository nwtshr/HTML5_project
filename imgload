    <script language="JavaScript">

    function init1(){
       a = new Date();
       a = "?" + a.getTime();
       newImage1 = document.imgload1.src;
       index1 = newImage1.indexOf("?", 0);
       if (index1 > 0)
       {
           newImage1 = newImage1.substr(0, index1);
       }
       document.imgload1.src = "image.jpg"+a;
    }
    
    
    function init2(){
        b = new Date();
        b = "?" + b.getTime();
        newImage2 = document.imgload2.src;
        index2 = newImage2.indexOf("?", 0);
        if (index2 > 0)
        {
            newImage2 = newImage2.substr(0, index2);
        }
        document.imgload2.src = "image.jpg"+b;
    }
    
    
